  <property name="styleSheet">
   <string notr="true">/* --- 1. ANA PENCERE (Derin Uzay Mavisi) --- */
QMainWindow {
    background-color: #10151e; /* Referans görseldeki koyu ton */
}

/* --- 2. YAZILAR (Okunabilir Parlak Gri) --- */
QLabel, QCheckBox, QRadioButton {
    color: #cfd8dc; 
    font-family: 'Segoe UI', sans-serif;
    font-size: 12px;
}

/* --- 3. GRUP KUTULARI (KIRMIZI YOK! Sadece Neon Mavi) --- */
QGroupBox {
    border: 1px solid #2a3b4c; /* Çerçeve: Çok silik mavi-gri */
    border-radius: 10px;       /* Yumuşak köşeler */
    margin-top: 22px; 
    background-color: #161e2b; /* Kutunun içi: Ana fondan bir tık açık */
}

QGroupBox::title {
    subcontrol-origin: margin;
    left: 15px;
    padding: 0 5px;
    color: #00e5ff; /* Başlıklar: NEON CYAN (Referans görseldeki renk) */
    font-size: 13px;
    font-weight: bold;
}

/* --- 4. BUTONLAR (Referanstaki O Parlak Mavi Geçiş) --- */
QPushButton {
    /* Mavi Gradient: Üstten alta koyulaşan profesyonel ton */
    background-color: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop:0 #0288d1, stop:1 #01579b);
    color: white;
    border: 1px solid #0288d1; /* Kendi renginde çerçeve */
    border-radius: 6px;        /* Görseldeki gibi hafif yuvarlak */
    padding: 6px;
    font-weight: bold;
    font-size: 12px;
}

QPushButton:hover {
    /* Üzerine gelince parlasın (Cyan'a kaysın) */
    background-color: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop:0 #29b6f6, stop:1 #0277bd);
    border: 1px solid #4fc3f7;
}

QPushButton:pressed {
    background-color: #01457b; /* Tıklayınca koyulaşsın */
    padding-top: 8px;
}

/* --- 5. ÖZEL BUTONLAR --- */

/* &quot;Sinyal Ekle&quot;, &quot;Gürültü Ekle&quot; gibi aksiyon butonları (Turkuaz) */
/* Eğer ObjectName'leri belliyse buraya ekleyebilirsin, yoksa genel buton stili iş görür */

/* KAYIT BUTONU (Hala Kırmızı Kalsın, Dikkat Çeksin) */
#btnRecord {
    background-color: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop:0 #ff5252, stop:1 #d32f2f);
    border: 1px solid #ff1744;
}
#btnRecord:hover {
    background-color: #ff867f;
}

/* --- 6. GİRDİ KUTULARI (Textbox &amp; Combobox) --- */
QLineEdit, QComboBox, QSpinBox, QDoubleSpinBox {
    background-color: #0d1219; /* Çok koyu zemin */
    border: 1px solid #37474f;
    border-radius: 4px;
    color: #00e5ff; /* İçindeki rakamlar Neon Cyan */
    padding: 4px;
}

/* --- 7. SLIDER (Modern Çubuk) --- */
QSlider::groove:horizontal {
    border: 1px solid #37474f;
    height: 6px;
    background: #0d1219;
    margin: 2px 0;
    border-radius: 3px;
}
QSlider::handle:horizontal {
    background: #00e5ff; /* Tutamaç: Neon Cyan */
    border: 1px solid #00e5ff;
    width: 14px;
    height: 14px;
    margin: -5px 0;
    border-radius: 7px;
}</string>
